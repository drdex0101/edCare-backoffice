CREATE TABLE admin (
    id BIGSERIAL , -- 會員ID (主鍵，自動遞增)
    account VARCHAR,
    email VARCHAR,         -- 電子郵件
    cellphone VARCHAR,
    site VARCHAR,
    is_enable BOOLEAN,
    password VARCHAR,
    role VARCHAR,
    created_ts TIMESTAMP DEFAULT CURRENT_TIMESTAMP, -- 建立時間
    updated_ts TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);
